{
  "name": "effitask",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run type-check",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "start": "npm-run-all --parallel dev start:server",
    "start:server": "node --loader ts-node/esm server.ts"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.3",
    "@types/firebase": "^3.2.1",
    "@vue-hero-icons/outline": "^1.7.2",
    "chart.js": "^4.4.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "emailjs-com": "^3.2.0",
    "express": "^4.19.2",
    "firebase": "^10.9.0",
    "nodemailer": "^6.9.13",
    "pinia": "^2.1.7",
    "qalendar": "^3.8.1",
    "sweetalert2": "^11.10.7",
    "vue": "^3.4.21",
    "vue-cal": "^4.8.1",
    "vue-router": "^4.3.0",
    "vue-sweetalert2": "^5.0.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.12.5",
    "@types/nodemailer": "^6.4.14",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.35",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.0",
    "vite": "^5.1.5",
    "vue-tsc": "^2.0.6"
  }
}
